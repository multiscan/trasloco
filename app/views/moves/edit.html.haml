= simple_form_for(@user, :url => move_path, :html => { :method => :put }) do |f|
  = f.error_notification
  = f.input :moving_from
  = f.input :moving_to
  = f.input :move_date
 
  = f.button :submit, class: "btn-primary"
  = f.button :button, "Cancel", type: "reset", class: "btn-outline-secondary"

